<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.freshmeat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.Store" />
        <activity android:name=".view.activity.Setting" />
        <activity android:name=".view.activity.Login" />
        <activity android:name=".view.activity.Favorite" />
        <activity android:name=".view.activity.Splash" />
        <activity android:name=".view.activity.SignUp" />
        <activity android:name=".view.activity.AdminProfile" />
        <activity android:name=".view.activity.CustomerProfile" />
        <activity android:name=".view.activity.ButcherProfile" />
        <activity android:name=".view.activity.AboutUs"></activity>
    </application>

</manifest>